{
    "configurations": [
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "type": "java",
            "name": "Spring Boot-Swagger2SpringBoot<swagger-spring>",
